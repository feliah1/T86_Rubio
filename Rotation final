#include <stdio.h>

int main()
{
   int var1 = 3000, var2 = 60, var3 = 360, result;

   printf(" var1 = 3000 \n ", var1);

   printf("var2 = 60 \n",var2);

   printf(" var3 = 360 \n", var3);


   //solution
   result = var1/var2*var3;
   printf("var1/ var2*var3 = %d RPM divide %d sec multiply %d degrees is = %d degrees per second \n", result);

   return 0;

}
